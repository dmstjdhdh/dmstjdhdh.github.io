{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "@typescript-eslint/ban-types": [
      "error",
      {
        "types": {
          "Function": false,
          "{}": false,
          "object": false
        },
        "extendDefaults": true
      }
    ]
  }
}
